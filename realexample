      sendClickFavoriteEvent(match, isFavorite) {
        utils.common.sendHackleEvent("click_like_match", isFavorite ? 1 : 0, {
          ...utils.common.getMatchEventInfo(match),
          previous_page: utils.common.getPreviousPath(),
          is_login: this.userInfo?.id ? true : false,
          user_level: this.userInfo ? utils.common.getRecordLevel(this.userInfo.profile_level.tier, this.userInfo.profile_level.grade) : null,
          user_level_name: this.userInfo ? `${this.userInfo.profile_level.tier_ko}${this.userInfo.profile_level.grade || ''}` : "none",
          is_earlybird: this.show_earlybird,
          like: isFavorite ? true : false,
        });
      },
      setFavorite(){
        let params = {
          is_active : !this.is_favorite_match
        }
        this.sendClickFavoriteEvent(this.matches, !this.is_favorite_match);

        axios.patch(`/api/v2/matches/${matchID}/favorite_match/`, params, config)
        .then((res) => {
          if(res.data.is_success){
            this.is_favorite_match = !this.is_favorite_match
            this.favorite_count += this.is_favorite_match ? 1 : -1
            if(this.is_favorite_match){
              this.$toast(`찜한 매치에 사람이 모이면 알려드릴게요.`, {
                duration: 3000,
                bottom: 105,

                actionLabel: "이동",
                actionUrl: `/mypage/favorite/match/`,
              });

              analytics.GA4.setEventForMatch(this.matches);
              analytics.GA4.sendEvent("add_to_wishlist");
            }
          }
        })
        .catch((err) => {console.log(err)})
      },
      setVacancyNotification() {
        let params = {
          "is_active": !this.is_vacancy_notification_applicant
        }
        axios.patch(`/api/v2/matches/${matchID}/vacancy_notification/`, params, config)
        .then((res) => {
          if (res.data.message == "NEED_LOGIN") {
            this.goLogin()
            return
          }

          if (res.data.is_success) {
            this.is_vacancy_notification_applicant = !this.is_vacancy_notification_applicant
            this.vacancy_applicants_count += this.is_vacancy_notification_applicant ? 1 : -1
            this.setVacancyNotificationButtonText(this.is_vacancy_notification_applicant)
            this.openToast()
          } else {
            alert("요청에 실패했어요. 채널톡으로 문의주세요.")
          }
        })
      },
      setVacancyNotificationButtonText(is_active) {
        this.vacancy_notification_button_text = is_active ? "알림 취소하기" : "알림 신청하기"
        this.vacancy_notification_toast_message = (
          is_active ? "빈자리 알림이 신청됐어요" : "빈자리 알림이 취소 됐어요"
        )
      },
      openToast() {
        this.$toast(this.vacancy_notification_toast_message, {
          duration: 3000,
          iconImgSrc: 'https://d31wz4d3hgve8q.cloudfront.net/static/img/ic_notice_color.svg',
        });
      },
      labe_late(apply_id){
        let apply = this.matches.applys.find(apply => apply.id == apply_id)
        let msg = ""
        if(apply != undefined){
          let tags = apply.tags
          tags.forEach(tag => {
            if(tag.code == 'LATE'){
              msg =  tag.value + "분 늦어요 🙏"
              return true
            }
          })
        }
        return msg
      },
      openInfoModal(item) {
        this.label = "";
        this.memo = "";
        if (item == "parking" && this.matches.parking_fee) {
          this.label = this.label_parking
          this.memo = this.matches.parking_fee
        }
        else if (item == "beverage" && this.matches.beverage_memo) {
          this.label = "음료 판매"
          this.memo = this.matches.beverage_memo
        }
        else if (item == "shower" && this.matches.shower_memo) {
          this.label = "샤워"
          this.memo = this.matches.shower_memo
        }
        else if (item == "shoes" && this.matches.shoes_memo) {
          this.label = "풋살화 대여"
          this.memo = this.matches.shoes_memo
        }
        else if (item == "toilet" && this.matches.toilet_memo) {
          this.label = this.label_toilet
          this.memo = this.matches.toilet_memo
        }
        else {
          return
        }
        this.show_info_modal = true
      },
