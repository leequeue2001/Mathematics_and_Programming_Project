        computed: {
          hideNavbar() {
            return (utils.app.isApp() && (window.location.pathname.includes('/team/registration/') || window.location.pathname.includes('/mypage/coupon/') || window.location.pathname.includes('/team/guest-matches/add/'))) || window.location.pathname.includes('/mypage/onboarding/');
          }
        },
        created() {
          if (utils.app.isApp()) {
            window.addEventListener('message',(e) => { this.handleEventListener(e)});
            document.addEventListener('message',(e) => { this.handleEventListener(e)});

            
