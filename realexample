    async created() {
      
      await this.fetchUser()
      await this.fetchMatches()
      
      
        this.show_rule = false
        this.show_refund = false
      
      this.fetchBanner()
      this.sendViewMatchDetailEvent();
      // 최근 접속한 매치 ID 기록
      window.sessionStorage.setItem("recent_mid",matchID)
      
      window.karrotPixel.track('ViewContent', {
        'id': matchID,
      });

      hackleClient.onReady(() => {
        if(this.matches.level === 3){
          const remoteConfig = hackleClient.remoteConfig();
          const targetMatches = remoteConfig.get("under_beginner3_test");
          
          this.underBeginner3TestMatchIds = JSON.parse(targetMatches)?.target_matches || [];
        }
      });
    },
    methods: {
      async fetchUser() {
        await axios.get('/api/v2/users/me/', config)
        .then((res) => {
          this.userInfo = res.data
          this.fetchCoupons()
        })
        .catch((err) => {
          console.log(err)
        })
      },
      async fetchMatches() {
        await axios.get(`/api/v2/matches/${matchID}/`, config)
        .then((res) => {
          this.matches = res.data
          this.leftPlayers = this.matches.max_player_cnt - this.matches.confirm_cnt
          if(this.matches.confirm_cnt < this.matches.min_player_cnt) {
            this.minLeftPlayers = this.matches.min_player_cnt - this.matches.confirm_cnt
          }

          // 빈자리 알림 신청 정보 조회
          if (this.matches.apply_status == "full") {
            this.fetchVacancyNotification()
          }

          if (this.matches.type != 'league') {
            this.fetchTeams()
            this.fetchExtraInfo()
          }

          //매치에 참여하는 친구 목록

          if(this.matches.type == 'league'){
            if(this.leftPlayers <= 0){
              this.matchStatusColor = '#1570FF'
              this.matchStatusMessage = '<a href="/league" style="color:' + this.matchStatusColor + ';font-weight:700; border-bottom: 2px solid;">' + '다음 일정을 미리 예약</a>하세요'
              this.matchStatusMessage += '<p style="font-size: 12px;">3일 전까지 무료 취소</p>'
            }
            else if(this.matches.confirm_cnt == 0){
              this.matchStatusColor = '#1570FF'
              cancel_date = moment(this.matches.schedule).subtract(1, 'days').format('D일')
              this.matchStatusMessage = '<font style="font-weight:700;">'+ [[cancel_date ]] +'까지 2팀 이상</font><br />신청하면 진행 확정'
            }
            else if(this.matches.confirm_cnt == 1){
              this.matchStatusColor = '#1570FF'
              cancel_date = moment(this.matches.schedule).subtract(1, 'days').format('D일')
              this.matchStatusMessage = '<font style="font-size: 14px;">'+[[cancel_date ]]+'까지 신청해야</font><br />진행이 확정돼요'
            }
            else if(this.matches.confirm_cnt >= 2){
              this.matchStatusColor = '#FF4D37'
              this.matchStatusMessage = '마감까지 <br/><font style="color:' + this.matchStatusColor + ';font-weight:700;">' + this.leftPlayers  + '자리 남았어요!</font>'
            }
            else{
              this.matchStatusMessage = '지금 신청하면<br/>진행 확정이 빨라져요!'
            }
          }
          else{
            if(this.leftPlayers  == 0){
              this.matchStatusColor = '#1570FF'
              this.matchStatusMessage = '<a href="/stadium/'+ this.matches.stadium_group_id + '/info/?tab=social" style="color:' + this.matchStatusColor + ';font-weight:700; border-bottom: 2px solid;">' + '다음 일정을 미리 예약</a>하세요'
              this.matchStatusMessage += '<p style="font-size: 12px;">2일 전까지 무료 취소</p>'
            } else if(this.matches.confirm_cnt >= 6 && this.matches.confirm_cnt < this.matches.min_player_cnt) {
              this.matchStatusColor = '#FEB700'
              this.matchStatusMessage = '진행 확정까지 <br/><font style="color:' + this.matchStatusColor + ';font-weight:700;">' + this.minLeftPlayers  + '자리 남았어요</font>'
            } else if (this.matches.confirm_cnt >= this.matches.min_player_cnt) {
              this.matchStatusColor = '#FF4D37'
              this.matchStatusMessage = '마감까지 <br/><font style="color:' + this.matchStatusColor + ';font-weight:700;">' + this.leftPlayers  + '자리 남았어요!</font>'
            } else{
              this.matchStatusMessage = '지금 신청하면<br/>진행 확정이 빨라져요!'
            }
          }

          if(this.matches.sex == 0 || this.matches.level == 1 || this.matches.level == 2 || this.matches.level == 5 || this.matches.test_type == 1 || this.matches.test_type == 4){
            this.showNotice = true;
          }

          if (this.matches.is_super_sub && this.userInfo.is_super_sub) {
            this.show_super_sub = true
            this.sale_price = 10000
          }
          else if (
            this.matches.is_manager_free &&
            this.matches.manager_id != 102 &&
            this.userInfo.is_manager_sub
          ) {
            this.show_manager_free = true
            this.sale_price = 10000
          }
          else if (this.matches.is_earlybird == 0 && this.matches.confirm_cnt < 4) {
            this.show_earlybird = true
            this.sale_price = 3000
          }
          else if (this.matches.is_earlybird == 1 && this.matches.confirm_cnt < 4) {
            this.show_earlybird = true
            this.sale_price = 5000
          }
          else if (this.matches.is_earlybird ==2 && this.matches.confirm_cnt < 3) {
            this.show_earlybird = true
            this.sale_price = 5000
          }
          else if (this.matches.is_earlybird ==2 && this.matches.confirm_cnt >= 3 && this.matches.confirm_cnt < 8) {
            this.show_earlybird = true
            this.sale_price = 3000
          }
          else {
            this.show_default = true
            this.sale_price = 0
          }
          if(this.matches.test_type == 4){
            this.show_nomanager_modal = true
          }
          this.matchStatus = true

          if(this.matches.type == "tshirt") {
            
            this.show_tshirt_open_modal = true
            
            this.fetchTshirtMatch()
            utils.component.show("match-comment","match/comment/comment",{'match' : this.matches, 'user_id' : user_id})
          }

          if(this.matches.type == 'starter'){
            utils.component.show("starter-section","match/starter")
            this.faqs.push({
              id: 1,
              subject: "완전 처음이어도 괜찮을까요?",
              content:
                "처음이어도 괜찮아요. 풋살을 경험하고 흥미를 느낄 수 있도록 코치님이 함께해요. 기본기를 배우고 경기도 같이 뛰어보세요.",
            });
            this.faqs.push({
              id: 2,
              subject: "풋살화가 있어야 하나요?",
              content:
                "아직은 빠른 동작이나 역동적인 움직임이 적기 때문에 풋살화 없이도 즐길 수 있어요. 다만 굽이 높거나 달리기에 적합하지 않은 운동화는 부상의 위험이 있기 때문에 꼭 편한 운동화를 착용하세요.",
            });
            this.faqs.push({
              id: 3,
              subject: "혼자 가도 괜찮을까요?",
              content:
                "플랩은 누구나 혼자 와도 즐길 수 있는 서비스를 지향하고 있습니다. 혼자 오셔도 어색하지 않도록 코치님이 도와드릴 거예요. 또 2인 1조로 메이트와 짝을 지어 드리기 때문에 혼자 와도 재밌게 할 수 있을 거예요!",
            });
            this.faqs.push({
              id: 4,
              subject: "커리큘럼이 어떻게 되나요?",
              content:
                "몸풀기 운동(웜업) - 몸풀기 게임 - 코디네이션 - 미니 게임 - 휴식 - 오늘의 훈련 - 게임 순서로 진행됩니다. 오늘의 훈련은 패스→드리블→슈팅→볼컨트롤 순서로 매주 바뀌어요.",
            });
          }
          
          if(this.matches.is_plaber_match){
            this.fetchLeadPlaberInfo()
          }
          this.fetchMatchViewer()
          
          this.fetchFriends()
          this.fetchFavorite()
          

          let scrollTo = utils.common.getParameterByName("scrollTo")
          if(scrollTo == "matchRule"){
            this.goToTshirtMatch("tshirt_matchRule")
          }
          else if (scrollTo == "matchLineup"){
            this.goToTshirtMatch("tshirt_matchLineup")
          }
          else if (scrollTo == "leagueMatchRule"){
            this.goToLeagueMatch("league_matchRule")
          }

          this.$nextTick(()=>{
            // League
            if (this.matches.type == 'league') {
              
              this.fetchMyTeams()
              
              this.fetchApplyTeams()
              this.show_super_sub = false;
              this.show_manager_free = false;
              this.show_earlybird = false;
              this.show_default = true;
              this.sale_price = 0;
            }
            else{
              this.fetchPlaystyle()
              this.fetchPlaberOfTheMatch()
            }
          });

          this.setGA4Event()
        })
        .catch((err) => {
          console.log(err)
        })
      },
      setGA4Event(){
        analytics.GA4.setEventForMatch(this.matches);
        analytics.GA4.sendEvent("view_item");
      },
      fetchMyTeams(){
        axios.get('/api/v2/teams/me/?view_type=my&page_size=30', config)
        .then((res) => {
          if (res.data.length > 0) {
            const filteredTeam = res.data.filter(function(team) {
              return (
                team.my_type == 'OWNER' || team.my_type == 'OPERATOR'
              )
            });
            this.my_managed_teams = filteredTeam;
          }
        })
      },
      fetchLevelGraphData(teamColor = null) {
        let url = `/api/v2/matches/${matchID}/level-graph/`;
        if (this.matches.type === 'tshirt' && teamColor) {
          url += `?team_color=${teamColor}`;
        }

        axios.get(url, config)
          .then((res) => {
            this.levelGraphData = res.data;
          });
      },
      fetchMatchViewer(){
        axios.get(`/api/v2/matches/${matchID}/viewer`, config)
        .then((res) => {
          if(res.data.result){
            this.viewer_count = res.data.viewer
            if(this.matches.confirm_cnt > this.viewer_count){
              this.viewer_count = this.matches.confirm_cnt
            }
          }
        })
      },
      fetchFavorite(){
        axios.get(`/api/v2/matches/${matchID}/favorite_match/`, config)
        .then((res) => {
          this.is_favorite_match = res.data.is_active
          this.favorite_count = res.data.total
        })
      },
      fetchVacancyNotification(){
        axios.get(`/api/v2/matches/${matchID}/vacancy_notification`, config)
        .then((res) => {
          this.is_vacancy_notification_applicant = res.data.is_vacancy_notification_applicant
          this.vacancy_applicants_count = res.data.vacancy_applicants_count
          this.setVacancyNotificationButtonText(this.is_vacancy_notification_applicant)
        })
      },
      fetchFriends(){
        axios.get(`/api/v2/matches/${matchID}/friends/`, config)
        .then((res) => {
          if(res.data.length > 0){
            Vue.set(this.matches,'friend_count',res.data[0].friend_count)
            Vue.set(this.matches,'friend_images',res.data[0].friend_images)
            if (res.data[0].friend_names) {
              let friend_name_list = res.data[0].friend_names.split(', ')
              const friend_names = friend_name_list.slice(0, 2).join(', ')
              res.data[0].friend_names = friend_names
            }
            Vue.set(this.matches,'friend_name',res.data[0].friend_name)
            Vue.set(this.matches,'friend_names',res.data[0].friend_names)
          }
        })
      },
      fetchBanner(){
        $(document).ready(function(){
          $('.contentHero').slick({
            dots: true,
            infinite: true,
            autoplay: true,
            autoplaySpeed: 2000,
            arrows: false,
            customPaging : function(slider, i) {
                var thumb = $(slider.$slides[i]).data();
                return '<div class="dots-inner"><a onclick="showGallery()"><strong>'+(i+1)+' </strong> |'+ slider.$slides.length+'</a></div>';
            },
            responsive: [
              {
              breakpoint: 768,
              settings: {
                  arrows: false,
                  centerMode: true,
                  centerPadding: '0px',
                  slidesToShow: 1
              }
              },
              {
              breakpoint: 480,
              settings: {
                  arrows: false,
                  centerMode: true,
                  centerPadding: '0px',
                  slidesToShow: 1
              }
              }
            ]
          });
        })
      },
      updateCountdown() {
          const t1 = moment(this.matches.schedule).subtract(30, 'minute').format()
          const t2 = moment()
          const t3 = moment(t1).diff(t2)
          if(moment(t3).format('DD') == '01') {
            this.countdown = moment(t3).utc().format('HH:mm:ss')
          } else if(moment(t3).format('DD') == '02') {
            this.countdown = moment(t3).utc().subtract(1, 'days').format('1일 h:mm:ss')
          } else {
            this.countdown = moment(t3).utc().format('D일 h:mm:ss')
          }
      },
      noCash() {
        var is_ok = confirm('캐시가 부족합니다. 캐시를 충전하시겠습니까?')
        if (is_ok) {
          utils.webView.navigate("/cash/charge/?matchId=" + matchID);
          this.$cookies.set('lm', '591326', '2h')
          this.$cookies.set('pt', 'social', '2h')
        }
      },
      fetchCoupons() {
        axios.get(`/api/v2/coupons/?match_id=${matchID}`, config)
        .then((res) => {
          if(res.data.results.length != 0) {
            this.coupons = res.data.results
            this.couponId = res.data.results[0].id
            this.fetchMyCoupons()
          }
        })
      },
      fetchMyCoupons() {
        axios.get('/api/v2/coupons/me/', config)
        .then((res) => {
          this.myCoupons = res.data
          for(i=0; i < res.data.length; i++) {
            if(res.data[i].coupon_type.id == this.couponId) {
              this.alreadyGetCoupon = true
            }
          }
        })
      },
      getCoupons() {
        
          if(this.alreadyGetCoupon) {
            alert('이미 쿠폰을 발급받았습니다')
          } else {
            axios.post('/api/v2/coupons/me/', {
              coupon_id: this.couponId
            }, config)
            .then((res) => {
              if(res.data.length != 0) {
                this.alreadyGetCoupon = true
                alert('쿠폰을 발급받았습니다')
              }
            })
            .catch((err) => {
              alert(err.response.data)
            })
          }
        
      },
      fetchTshirtMatch() {
        let light_playstyles = []
        let dark_playstyles = []
        let light = 0
        let dark = 0
        let dark_apply_count = 0
        let light_apply_count = 0
        for (var i = 0; i < this.matches.teams.length; i ++) {
          for (var j = 0; j < this.matches.teams[i].match_player.length; j++) {
            if (this.matches.teams[i].match_player[j].profile_level) {
              Vue.set(this.matches.teams[i].match_player[j], 'editLevel', this.matches.teams[i].match_player[j].profile_level)
            }
            else {
              Vue.set(this.matches.teams[i].match_player[j], 'editLevel', 0)
            }
            if (this.matches.teams[i].color == "LIGHT") {
              if (this.matches.teams[i].match_player[j].profile_level) {
                light = +light + +this.matches.teams[i].match_player[j].profile_level
                let k = this.levelGrade(this.matches.teams[i].match_player[j].profile_level)
                this.lightLevels[k+1].value = this.lightLevels[k+1].value + 1
                ++light_apply_count
              }
              else {
                this.lightLevels[0].value = this.lightLevels[0].value + 1
              }
              if (this.matches.teams[i].match_player[j].playstyle != undefined) {
                light_playstyles.push(this.setPlayStylePerPlayer(this.matches.teams[i].match_player[j]))
              }
            }
            else if (this.matches.teams[i].color == "DARK") {
              if (this.matches.teams[i].match_player[j].profile_level) {
                dark = +dark + +this.matches.teams[i].match_player[j].profile_level
                let k = this.levelGrade(this.matches.teams[i].match_player[j].profile_level)
                this.darkLevels[k+1].value = this.darkLevels[k+1].value + 1
                ++dark_apply_count
              }
              else {
                this.darkLevels[0].value = this.darkLevels[0].value + 1
              }
              if (this.matches.teams[i].match_player[j].playstyle != undefined) {
                dark_playstyles.push(this.setPlayStylePerPlayer(this.matches.teams[i].match_player[j]))
              }
            }
          }
        }
        this.tshirt_light_avg_level = light / light_apply_count
        this.tshirt_dark_avg_level = dark / dark_apply_count
        this.playStyleLightPlayers = new Array(8).fill(0)
        this.playStyleDarkPlayers = new Array(8).fill(0)
        this.setLightPlayStyleInfo(light_playstyles)
        this.setLightPlayStyleGraph()
        this.setDarkPlayStyleInfo(dark_playstyles)
        this.setDarkPlayStyleGraph()
        this.team_max_player_cnt = this.matches.max_player_cnt / 2;
      },
      fetchLeadPlaberInfo(){
        axios.get(`/api/v2/matches/${matchID}/plaber-match-info/`, config)
        .then((res) => {
          this.leadPlaberInfo = res.data;
        })
      },
      setLightPlayStyleInfo(playStyles){
        this.playStyleLightPlayers.fill(0)
        playStyles.forEach((item,idx) => {
          for(var i = 0; i< this.playStyleLightPlayers.length; i++){
            this.playStyleLightPlayers[i] += parseFloat(item[i])
          }
        })
        for(var i = 0; i< this.playStyleLightPlayers.length; i++){
          this.playStyleLightPlayers[i]  = this.playStyleLightPlayers[i] / playStyles.length
        }
      },
      setLightPlayStyleGraph(){
        playStyleChart = new Chart(document.getElementById("myLightChart"), {
          type: 'radar',
          data: {
            labels: ["공격", "스피드", "드리블", "체력", "수비","피지컬","패스","슛"],
            datasets: [
              {
                label: "매치",
                fill: false,
                borderColor:  this.playStyle_team == null ? "rgba(21, 112, 255, 1)" : "rgba(255, 77, 55, 1)",
                pointBorderColor:  this.playStyle_team == null ? "rgba(21, 112, 255, 1)" : "rgba(255, 77, 55, 1)",
                data: this.playStyleLightPlayers,
                pointStyle : 'line',
              },
              {
                label: "전체",
                fill: false,
                borderColor: "rgba(254, 208, 20, 1)",
                pointBorderColor: "rgba(254, 208, 20, 1)",
                data: this.playStyleAllUsers,
                pointStyle : 'line',
              }
            ],
          },
          options: {
            title: {
              display: false,
            },
            plugins : {
              legend : {
                display : false,
              },
            },
            scales : {
              r : {
                suggestedMin: 0,
                suggestedMax: 10,
                stepSize: 2,
                grid: {
                    color: ['rgba(247, 255, 233, 1)','rgba(247, 255, 233, 1)',
                            'rgba(219, 255, 193, 1)','rgba(219, 255, 193, 1)',
                            'rgba(165, 255, 151, 1)','rgba(165, 255, 151, 1)',
                            'rgba(0, 231, 134, 1)','rgba(0, 231, 134, 1)',
                            'rgba(0, 203, 127, 1)','rgba(0, 203, 127, 1)'],
                    lineWidth : 15,
                },
                ticks: {
                  color : 'rgba(0,0,0,0)',
                  showLabelBackdrop : false
                },
                pointLabels: {
                  color: 'black',
                  padding : 10,
                  font : {
                    size : 12
                  }
                }
              }
            }
          }
        });
      },
      setDarkPlayStyleInfo(playStyles){
        this.playStyleDarkPlayers.fill(0)
        playStyles.forEach((item,idx) => {
          for(var i = 0; i< this.playStyleDarkPlayers.length; i++){
            this.playStyleDarkPlayers[i] += parseFloat(item[i])
          }
        })
        for(var i = 0; i< this.playStyleDarkPlayers.length; i++){
          this.playStyleDarkPlayers[i]  = this.playStyleDarkPlayers[i] / playStyles.length
        }
      },
      setDarkPlayStyleGraph(){
        playStyleChart = new Chart(document.getElementById("myDarkChart"), {
          type: 'radar',
          data: {
            labels: ["공격", "스피드", "드리블", "체력", "수비","피지컬","패스","슛"],
            datasets: [
              {
                label: "매치",
                fill: false,
                borderColor:  this.playStyle_team == null ? "rgba(21, 112, 255, 1)" : "rgba(255, 77, 55, 1)",
                pointBorderColor:  this.playStyle_team == null ? "rgba(21, 112, 255, 1)" : "rgba(255, 77, 55, 1)",
                data: this.playStyleDarkPlayers,
                pointStyle : 'line',
              },
              {
                label: "전체",
                fill: false,
                borderColor: "rgba(254, 208, 20, 1)",
                pointBorderColor: "rgba(254, 208, 20, 1)",
                data: this.playStyleAllUsers,
                pointStyle : 'line',
              }
            ],
          },
          options: {
            title: {
              display: false,
            },
            plugins : {
              legend : {
                display : false,
              },
            },
            scales : {
              r : {
                suggestedMin: 0,
                suggestedMax: 10,
                stepSize: 2,
                grid: {
                    color: ['rgba(247, 255, 233, 1)','rgba(247, 255, 233, 1)',
                            'rgba(219, 255, 193, 1)','rgba(219, 255, 193, 1)',
                            'rgba(165, 255, 151, 1)','rgba(165, 255, 151, 1)',
                            'rgba(0, 231, 134, 1)','rgba(0, 231, 134, 1)',
                            'rgba(0, 203, 127, 1)','rgba(0, 203, 127, 1)'],
                    lineWidth : 15,
                },
                ticks: {
                  color : 'rgba(0,0,0,0)',
                  showLabelBackdrop : false
                },
                pointLabels: {
                  color: 'black',
                  padding : 10,
                  font : {
                    size : 12
                  }
                }
              }
            }
          }
        });
      },
      fetchPlaystyle() {
        this.players = this.matches.applys
        var playStyles = []
        for(i=0; i < this.players.length; i++) {
          if(this.players[i].status === 'CONFIRM') {
            let editLV = this.players[i].level
            if (editLV) {
              Vue.set(this.players[i], 'editLevel', editLV)
              this.editAvgLevel = +this.editAvgLevel+editLV
              this.applysPlayer ++
            } else {
              Vue.set(this.players[i], 'editLevel', 0)
              this.editAvgLevel = this.editAvgLevel+this.players[i].playstyle.level
              this.applysPlayer ++
            }
            //공격 스피드 드리블 체력 수비 피지컬 패스 슛
            if(this.players[i].playstyle.style != undefined){
                playStyles.push(this.setPlayStylePerPlayer(this.players[i]))
            }
          }
        }
        this.editAvgLevel = this.editAvgLevel/this.applysPlayer
        this.playStylePlayers = new Array(8).fill(0)
        this.fetchLevelGraphData()
        this.setPlayStyleInfo(playStyles)
        this.$nextTick(()=>{
          this.setPlayStyleGraph()
        })
      },
      setPlayStyleInfo(playStyles){
        this.playStylePlayers.fill(0)
        playStyles.forEach((item,idx) => {
          for(var i = 0; i< this.playStylePlayers.length; i++){
            this.playStylePlayers[i] += parseFloat(item[i])
          }
        })
        for(var i = 0; i< this.playStylePlayers.length; i++){
          this.playStylePlayers[i]  = this.playStylePlayers[i] / playStyles.length
        }
      },
      setPlayStylePerPlayer(player){
        let style = player.playstyle.style
        let strength = player.playstyle.strength
        let baseLevel = player.editLevel == 0 ? player.playstyle.level : player.editLevel
        let playStyle = new Array(8)
        for(let i = 0; i < playStyle.length; i++){
          playStyle[i] = baseLevel * 1.4
        }
        if(style == "ATTACK"){
          playStyle[0] = baseLevel * 2.0
        }
        else if(style == "DEFENCE") {
          playStyle[4] = baseLevel * 2.0
        }
        else{
          playStyle[0] = baseLevel * 1.75
          playStyle[4] = baseLevel * 1.75
        }
        if(strength == "DRIBBLE"){
          playStyle[2] = baseLevel * 2.0
        }
        else if(strength == "PASS"){
          playStyle[6] = baseLevel * 2.0
        }
        else if(strength == "PHYSICAL"){
          playStyle[5] = baseLevel * 2.0
        }
        else if(strength == "SHOOT"){
          playStyle[7] = baseLevel * 2.0
        }
        else if(strength == "SPEED"){
          playStyle[1] = baseLevel * 2.0
        }
        else if(strength == "STAMINA"){
          playStyle[3] = baseLevel * 2.0
        }
        return playStyle
      },
      levelGrade(level){
        // ["루키","스타터","비기너","아마추어","세미프로","프로"] 의 인덱스를 return
        if (!level && this.matches.type == "tshirt") {
          return 0
        }
        if(level != 0){
          let avgLevel =  parseInt((parseFloat(level) - 0.1).toFixed(1))
          return avgLevel
        }
        else{
          return 0
        }
      },
      validationApply(){
        axios.get(`/api/v2/matches/${matchID}/validation`, config)
        .then((res) => {
          if(this.matches.level === 3 && this.underBeginner3TestMatchIds.includes(this.matches.id)){
            const allowedTiers = ["BEGINNER", "ROOKIE"];
            if (!allowedTiers.includes(this.userInfo.profile_level.tier)) {
              this.$toast(`비기너3 이하 플래버만 신청할 수 있어요`)
              return;
            }
          }

          if (res.data.result) {
            if(!this.validateProfileName(this.userInfo.name)){
              this.openProfileNameModifyModal();
              window.ChannelIO("hideChannelButton");
            }
            else if(this.matches.type == "tshirt"){
              if (this.matches.manager_id == 102
                && moment(this.matches.schedule).diff(moment(),'minutes') > 180
                && this.matches.is_manager_free
                && this.userInfo.is_manager_sub){
                this.goApply()
              }
              else{
                this.show_tshirt_modal = true
                window.ChannelIO("hideChannelButton");
              }
            }
            else if(this.matches.is_plaber_match){
                this.openPlaberMatchModal();
            }
            else if (this.matches.type == 'league') {
              if (this.my_managed_teams?.length > 0) {
                this.team_code = ""
                this.show_league_apply_modal = true;
                window.ChannelIO("hideChannelButton");
              }
              else {
                is_ok = confirm("신청가능한 팀이 없어요. 팀을 만들까요?")
                if (is_ok) {
                  team.util.openRegistTeamModal();
                }
              }
            }
            else if([120, 26].includes(stadium_group_id)){
              // 은평 롯데몰(26), HM풋살파크 청라(120)의 경우 소셜 매치 신청 시 금연 주의 팝업 노출
              this.show_smoking_warning_modal = true;
              window.ChannelIO("hideChannelButton");
            }
            else if(stadium_group_id === 3638){
              // 서경대(3638)의 경우 소셜 매치 신청 시 구장 내 주의 팝업 노출
              this.showWarningModal = true
              window.ChannelIO("hideChannelButton");
            }
            else{
              this.goApply()
            }
          }
          else {
            if (res.data.message == "StadiumBlack") {
              alert('불특정 다수의 반복된 신고와 구장측의 요구로 해당 구장의 참여가 제한됩니다')
              utils.webView.navigate("/")
            }
            else if (res.data.message == "SoldOut") {
              alert('마감된 매치입니다')
              location.reload()
            }
            else if (res.data.message == "TimeOut") {
              alert('종료된 매치입니다')
              utils.webView.navigate("/")
            }
            else if (res.data.message == "OverdueApply") {
              alert('신청이 마감됐어요')
              utils.webView.navigate("/")
            }
            else if (res.data.message == "AlreadyApply") {
              alert('이미 신청한 내역이 있습니다')
              utils.webView.navigate("/")
            }
            else if (res.data.message == "BlackList") {
              utils.webView.navigate("/")
            }
            else if (res.data.message == "RefundInProgress") {
              utils.webView.navigate("/")
            }
            else if (res.data.message == "IncorrectSex") {
              if (this.matches.sex == 1) {
                alert('남자만 신청 가능한 매치입니다. 남자이시면 \'MY->설정\'에서 성별을 남자로 변경 후 신청해 주세요!')
              }
              else {
                alert('여자만 신청 가능한 매치입니다. 여자이시면 \'MY->설정\'에서 성별을 여자로 변경 후 신청해 주세요!')
              }
            }
            else if (res.data.message == "PenaltyCard") {
              alert('페널티카드 내역이 없는 플래버만 참여가 가능합니다')
            }
            else if (res.data.message == "PenaltyCardSupersub") {
              var is_ok = confirm('옐로카드가 있으면, 슈퍼서브를 사용할 수 없어요.\r캐시로 참여할까요?')
              if (is_ok) {
                this.goApply()
              }
            }
            else if (res.data.message == "ConsecutiveSupersub") {
              var is_ok = confirm('연속된 일정의 슈퍼서브는 사용할 수 없어요.\r캐시로 참여할까요?')
              if (is_ok) {
                this.goApply()
              }
            }
            else if (res.data.message == "NoCash"){
              this.noCash()
            }
            else if (res.data.message == "LowLevel"){
              alert(`${this.label_level}만 참여 가능한 매치입니다.`)
            }
            else if (res.data.message == "NonLinkManagerAppAccount"){
              alert("매니저서브를 신청하려면 매니저 앱 > 마이페이지에서 플랩풋볼 계정을 연동해 주세요")
            }
          }
        })
      },
      goApply() {
        let url = "/match/591326/order/"
        if (this.tshirt_selected_team.id != null){
          url += "?team_id=" + this.tshirt_selected_team.id
        }
        if (this.team_code) {
          url += "?team_code=" + this.team_code
        }
        if(this.matches.is_plaber_match){
          url += "?is_leader=" + this.leadPlaberValue
        }
        utils.webView.navigate(url);
      },
      convertFee(fee){
        let fee_str = ""
        if(!isNaN(fee)){
          fee_str = fee + ""
        }
        return fee_str.replace(/\B(?=(\d{3})+(?!\d))/g, ",") + "원"
      },
      matchLabelText() {
        now = moment(new Date())
        schedule = moment(this.matches.schedule)
        duration = moment.duration(schedule.diff(now))
        if (this.matches.is_badweather_sale) {
          time_diff = duration.asMinutes()
          if ([1,2].includes(this.matches.grade)) {
            return "마감이 빠른 매치예요. 신청을 서두르세요 ⚡️"
          }
          if ([3,null].includes(this.matches.grade) && time_diff >= 90) {
            return "미리 신청하세요. 매치 시작 1시간 30분 전까지 무료로 취소할 수 있어요"
          }
          if ([4,5].includes(this.matches.grade) && !this.show_earlybird && time_diff >= 90) {
            return "미리 신청하세요. 매치 시작 1시간 30분 전까지 무료로 취소할 수 있어요"
          }
          if ([4,5].includes(this.matches.grade) && this.show_earlybird && time_diff >= 90) {
            return "할인 혜택을 놓치지 마세요. 매치 시작 1시간 30분 전까지 무료로 취소할 수 있어요"
          }
        }
        else {
          day_diff = duration.days()
          cancel_date = moment(this.matches.schedule).subtract(2, 'days').format('M월 D일')
          if ([1,2].includes(this.matches.grade)) {
            return "마감이 빠른 매치예요. 신청을 서두르세요 ⚡️"
          }
          if ([3,null].includes(this.matches.grade) && day_diff >= 2) {
            return "미리 신청하세요. "+cancel_date+ " 까지 무료로 취소할 수 있어요"
          }
          else if ([3,null].includes(this.matches.grade) && day_diff < 2) {
            return "미리 신청하면 진행 확정이 빨라져요!"
          }
          if ([4,5].includes(this.matches.grade) && !this.show_earlybird  && day_diff >= 2) {
            return "미리 신청하세요. "+cancel_date+ " 까지 무료로 취소할 수 있어요"
          }
          else if ([4,5].includes(this.matches.grade) && !this.show_earlybird  && day_diff < 2) {
            return "미리 신청하면 진행 확정이 빨라져요!"
          }
          if ([4,5].includes(this.matches.grade) && this.show_earlybird && day_diff >= 2) {
            return "할인 혜택을 놓치지 마세요. "+cancel_date+ " 까지 무료로 취소할 수 있어요"
          }
          else if ([4,5].includes(this.matches.grade) && this.show_earlybird && day_diff < 2) {
            return "선착순으로 할인 혜택이 종료돼요!"
          }
        }
      },
      goPlabLevel(){
        utils.webView.openInAppBrowser("/magazine/1/");
      },
