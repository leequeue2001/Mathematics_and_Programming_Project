        if(level!= 0){
          return utils.common.getLevelText(level)
        }
        else{
          return ""
        }
      },
      setPlayStyleGraph(){
        playStyleChart = new Chart(document.getElementById("myChart"), {
          type: 'radar',
          data: {
            labels: ["공격", "스피드", "드리블", "체력", "수비","피지컬","패스","슛"],
            datasets: [
              {
                label: "매치",
                fill: false,
                borderColor:  this.playStyle_team == null ? "rgba(21, 112, 255, 1)" : "rgba(255, 77, 55, 1)",
                pointBorderColor:  this.playStyle_team == null ? "rgba(21, 112, 255, 1)" : "rgba(255, 77, 55, 1)",
                data: this.playStylePlayers,
                pointStyle : 'line',
              },
              {
                label: "전체",
                fill: false,
                borderColor: "rgba(254, 208, 20, 1)",
                pointBorderColor: "rgba(254, 208, 20, 1)",
                data: this.playStyleAllUsers,
                pointStyle : 'line',
              }
            ],
          },
          options: {
            title: {
              display: false,
            },
            plugins : {
              legend : {
                display : false,
              },
            },
            scales : {
              r : {
                suggestedMin: 0,
                suggestedMax: 10,
                stepSize: 2,
                grid: {
                    color: ['rgba(247, 255, 233, 1)','rgba(247, 255, 233, 1)',
                            'rgba(219, 255, 193, 1)','rgba(219, 255, 193, 1)',
                            'rgba(165, 255, 151, 1)','rgba(165, 255, 151, 1)',
                            'rgba(0, 231, 134, 1)','rgba(0, 231, 134, 1)',
                            'rgba(0, 203, 127, 1)','rgba(0, 203, 127, 1)'],
                    lineWidth : 15,
                },
                ticks: {
                  color : 'rgba(0,0,0,0)',
                  showLabelBackdrop : false
                },
                pointLabels: {
                  color: 'black',
                  padding : 10,
                  font : {
                    size : 12
                  }
                }
              }
            }
          }
        });
      },
      updatePlayStyleGraph(){
        playStyleChart.data.datasets.splice(0,playStyleChart.data.datasets.length)
        playStyleChart.data.datasets.push(
          {
            label: "매치",
            fill: false,
            borderColor: this.playStyle_team == null ? "rgba(21, 112, 255, 1)" : "rgba(255, 77, 55, 1)",
            pointBorderColor: this.playStyle_team == null ? "rgba(21, 112, 255, 1)" : "rgba(255, 77, 55, 1)",
            data: this.playStylePlayers,
            pointStyle : 'line',
          },
          {
            label: "전체",
            fill: false,
            borderColor: "rgba(254, 208, 20, 1)",
            pointBorderColor: "rgba(254, 208, 20, 1)",
            data: this.playStyleAllUsers,
            pointStyle : 'line',
          }
        )
        playStyleChart.update()
      },
