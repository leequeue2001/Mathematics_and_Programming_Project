      validateProfileName(currentName){
        currentName = currentName?.replace(/\s/g, "");
        const regex = /^[a-zA-Z가-힣]{2,10}$/;
        return regex.test(currentName);
      },
      isShowMatchPoint(){
        let show_match_types = ['match','3teams','starter','league']
        if(show_match_types.indexOf(this.matches.type) > -1){
          return true
        }
        return false
      },
      isoDateToKST(isoDate, formatType) {
        return utils.common.isoDateToKST(isoDate, formatType);
      },
      // 리그 관련
      fetchApplyTeams() {
        axios.get(`/api/v2/league/${league_id}/teams`, config)
        .then((res) => {
          this.league_apply_teams = res.data
          if(this.matches.is_start == false){
            if(this.league_apply_teams.length>0){
              this.league_current_team = this.league_apply_teams[0]
              for(let team of this.league_apply_teams){
                let team_record = ""
                let team_win = 0
                let team_draw = 0
                let team_lose = 0
                team.record.map(obj => {team_win += obj.win; team_draw += obj.draw; team_lose += obj.lose});
                if(team_win > 0){
                  team_record+= team_win+"승 "
                }
                if(team_draw > 0){
                  team_record+= team_draw+"무 "
                }
                if(team_lose > 0){
                  team_record+= team_lose+"패 "
                }

                if(team_record == ""){
                  team_record ="-"
                }
                else{
                  team_record = team_record.slice(0, -1);
                }

                Vue.set(team,'label_record',team_record)

                // 그래프를 위한 스텟 정리
                team.stats = [
                  team.stat_attack_point,
                  team.stat_speed_point,
                  team.stat_dribble_point,
                  team.stat_stamina_point,
                  team.stat_defence_point,
                  team.stat_physical_point,
                  team.stat_pass_point,
                  team.stat_shoot_point,
                ];
              }
              this.$nextTick(()=>{
                this.setLeagueTeamChart(this.league_current_team.stats)
              })
            }
          }
          this.league_apply_team_ids = res.data.map((team) => team.team_id);

          if(this.league_tournament.match_ids.includes(this.matches.id)){
            this.fetchLeagueInfoForTournament()
            setInterval(() => this.fetchLeagueInfoForTournament(),3000 * 60);
          }
          else{
            this.fetchStandings()
            this.fetchFixtures()
          }
        })
      },
      fetchLeagueInfoForTournament(){
        this.fetchStandings()
        this.fetchFixtures()
        this.fetchChuckcheckEventResults()
      },
      fetchFixtures() {
        const v = this;
        axios.get(`/api/v2/league/${league_id}/fixtures/`, config)
        .then((res) => {
          if (res.data?.length > 0) {
            this.fixtures = res.data
            if(this.league_tournament.match_id == this.matches.id){
              this.league_tournament.semifinal_a_1_team = this.fixtures[0].home
              this.league_tournament.semifinal_a_2_team = this.fixtures[0].away
              this.league_tournament.semifinal_a_1_team.score = this.fixtures[0].home_score
              this.league_tournament.semifinal_a_2_team.score = this.fixtures[0].away_score
              this.league_tournament.semifinal_b_1_team = this.fixtures[1].home
              this.league_tournament.semifinal_b_2_team = this.fixtures[1].away
              this.league_tournament.semifinal_b_1_team.score = this.fixtures[1].home_score
              this.league_tournament.semifinal_b_2_team.score = this.fixtures[1].away_score

              if(this.fixtures.length >= 4){
                this.league_tournament.third_1_team = this.fixtures[2].home
                this.league_tournament.third_2_team = this.fixtures[2].away
                this.league_tournament.third_1_team.score = this.fixtures[2].home_score
                this.league_tournament.third_2_team.score = this.fixtures[2].away_score
                this.league_tournament.final_1_team = this.fixtures[3].home
                this.league_tournament.final_2_team = this.fixtures[3].away
                this.league_tournament.final_1_team.score = this.fixtures[3].home_score
                this.league_tournament.final_2_team.score = this.fixtures[3].away_score
              }
            }
          }
          else {
            if (moment(this.matches.schedule).format("mm") == "59") {
              this.matches.schedule = moment(this.matches.schedule).add(1, "minute")
            }
            home_array = ["A","A","B","B","C","C","A","A","B"]
            away_array = ["B","C","C","A","A","B","B","C","C"]
            var start_time = this.matches.schedule
            for (let i=0; i<home_array.length; i++) {
              var minute = 13
              if(i == 0) minute = 0
              else if(i % 3 == 0) minute = 14
              start_time = moment(start_time).add(minute, "minute")
              this.fixtures.push(
                {
                  home: {
                    name: home_array[i]+"팀",
                  },
                  away: {
                    name: away_array[i]+"팀",
                  },
                  score_away: null,
                  score_home: null,
                  status: "READY",
                  start_time: start_time.format("HH시 mm분"),
                  winTeam: null,
                },
              )
            }
            this.fixtures.forEach((value, index, arr) => {
              if (value.home.name === "A팀" && this.league_apply_teams[0]) {
                arr[index].home = this.league_apply_teams[0]
              }
              else if (value.home.name === "B팀" && this.league_apply_teams[1]) {
                arr[index].home = this.league_apply_teams[1]
              }
              else if (value.home.name === "C팀" && this.league_apply_teams[2]) {
                arr[index].home = this.league_apply_teams[2]
              }
              if (value.away.name === "A팀" && this.league_apply_teams[0]) {
                arr[index].away = this.league_apply_teams[0]
              }
              else if (value.away.name === "B팀" && this.league_apply_teams[1]) {
                arr[index].away = this.league_apply_teams[1]
              }
              else if (value.away.name === "C팀" && this.league_apply_teams[2]) {
                arr[index].away = this.league_apply_teams[2]
              }
            });
          }
        })
      },
      fetchChuckcheckEventResults(){
        const TOURNAMENT_A_LEAGUE_ID = 1206;
        const TOURNAMENT_B_LEAGUE_ID = 1207;

        axios.get(`/api/v2/league/${TOURNAMENT_A_LEAGUE_ID}/fixtures/`, config)
        .then(({data}) => {
          this.chuckcheck_event.quarterfinals_A = { ...data[0].home, score: data[0].home_score };
          this.chuckcheck_event.quarterfinals_B = { ...data[0].away, score: data[0].away_score };
          this.chuckcheck_event.quarterfinals_C = { ...data[1].home, score: data[1].home_score };
          this.chuckcheck_event.quarterfinals_D = { ...data[1].away, score: data[1].away_score };

          if(data.length >= 3){
            this.chuckcheck_event.semifinals_A = {...data[2].home, score: data[2].home_score};
            this.chuckcheck_event.semifinals_B = {...data[2].away, score: data[2].away_score};
          }
          if(data.length === 4){
            this.chuckcheck_event.final_A = {...data[3].home, score: data[3].home_score};
            this.chuckcheck_event.final_B = {...data[3].away, score: data[3].away_score};
          }
        })

        axios.get(`/api/v2/league/${TOURNAMENT_B_LEAGUE_ID}/fixtures/`, config)
        .then(({data}) => {
          this.chuckcheck_event.quarterfinals_E = { ...data[0].home, score: data[0].home_score };
          this.chuckcheck_event.quarterfinals_F = { ...data[0].away, score: data[0].away_score };
          this.chuckcheck_event.quarterfinals_G = { ...data[1].home, score: data[1].home_score };
          this.chuckcheck_event.quarterfinals_H = { ...data[1].away, score: data[1].away_score };

          if(data.length >= 3){
            this.chuckcheck_event.semifinals_C = {...data[2].home, score: data[2].home_score};
            this.chuckcheck_event.semifinals_D = {...data[2].away, score: data[2].away_score};
          }
        })

      },
      fetchStandings() {
        axios.get(`/api/v2/league/${league_id}/standings/`, config)
        .then((res) => {
          if (res.data?.length > 0) {
            this.standings = res.data
            var rank = 1
            var before_item  = this.standings[0]
            Vue.set(before_item,'rank',rank)
            for(const standing of this.standings){
              if(before_item.points == standing.points && before_item.goal_difference == standing.goal_difference){
                rank = rank
              }
              else{
                rank = rank + 1
                before_item=standing
              }
              Vue.set(standing,'rank',rank)
            }
          }
          else {
            const alphabets = ["A", "B", "C"]
            for (let i=0; i<alphabets.length; i++) {
              this.standings.push({
                rank: null,
                team: {
                  name: alphabets[i]+"팀",
                },
                matches_won: null,
                matches_drawn: null,
                matches_lost: null,
                goal_difference: null,
                points: null,
              })
            }
            this.standings.forEach((value, index, arr) => {
              if (value.team.name === "A팀" && this.league_apply_teams[0]) {
                arr[index].team = this.league_apply_teams[0]
              }
              else if (value.team.name === "B팀" && this.league_apply_teams[1]) {
                arr[index].team = this.league_apply_teams[1]
              }
              if (value.team.name === "C팀" && this.league_apply_teams[2]) {
                arr[index].team = this.league_apply_teams[2]
              }
            });
          }
        })
      },
      selectLeaguePreviewTeam(team){
        this.league_current_team = team
        this.updateLeagueTeamChart(this.league_current_team.stats)
      },
      goTeamProfile(code) {
        if (!code) return;
        utils.webView.navigate(`/team/profile/${code}`);
      },
      goStadiumPage(stadium_group_id){
        if (!stadium_group_id) return;
        utils.webView.navigate(`/stadium/${stadium_group_id}/info/`);
      },
      setLeagueTeamChart(data){
        if(this.specialMatchId.includes(matchID)){
          return
        }
        leagueTeamChart = new Chart(document.getElementById("leagueTeamChart"), {
          type: 'radar',
          data: {
            labels: [
              "공격",
              "스피드",
              "드리블",
              "체력",
              "수비",
              "피지컬",
              "패스",
              "슛",
            ],
            datasets: [
              {
                label: "전체",
                borderColor: "rgba(21, 112, 255, 1)",
                pointBorderColor: "rgba(21, 112, 255, 1)",
                data: data,
                fill: true,
                pointStyle: "line",
              },
            ],
          },
          options: {
            title: {
              display: false,
            },
            plugins : {
              legend : {
                display : false,
              },
            },
            scales : {
              r : {
                suggestedMin: 1,
                suggestedMax: 10,
                stepSize: 2,
                grid: {
                  color: [
                    "rgba(247, 255, 233, 1)",
                    "rgba(217, 255, 202, 1)",
                    "rgba(145, 255, 165, 1)",
                    "rgba(0, 235, 144, 1)",
                    "rgba(0, 210, 129, 1)",
                  ],
                  lineWidth: 15,
                },
                ticks: {
                  color: "rgba(0,0,0,0)",
                  showLabelBackdrop: false,
                },
                pointLabels: {
                  color: "rgba(40, 43, 51, 1)",
                  padding: 4,
                  font: {
                    size: 12,
                  },
                },
              }
            }
          }
        });
      },
      updateLeagueTeamChart(data){
        if(this.specialMatchId.includes(matchID)){
          return
        }
        leagueTeamChart.data.datasets.splice(0,leagueTeamChart.data.datasets.length)
        leagueTeamChart.data.datasets.push(
          {
            label: "전체",
            borderColor: "rgba(21, 112, 255, 1)",
            pointBorderColor: "rgba(21, 112, 255, 1)",
            data: data,
            fill: true,
            pointStyle: "line",
          },
        )
        leagueTeamChart.update()
      },
      isWinnerMatch(my_team,agaist_team, type){
        if(my_team != null && agaist_team != null){
          if(my_team.score > agaist_team.score){
            return true
          }
          else if(my_team.score === agaist_team.score){
            if(type === 'quarter'){
              return (
                this.chuckcheck_event.semifinals_A?.id === my_team.id ||
                this.chuckcheck_event.semifinals_B?.id === my_team.id ||
                this.chuckcheck_event.semifinals_C?.id === my_team.id ||
                this.chuckcheck_event.semifinals_D?.id === my_team.id
              )
            } else if (type === 'semi') {
              return (
                this.chuckcheck_event.final_A?.id === my_team.id ||
                this.chuckcheck_event.final_B?.id === my_team.id
              )
              return false
            }
          } else {
            return false
          }
        }
      },
      openTeamSelectModalForAttendanceVote(){
        utils.modal.show(
          "team-select-modal-for-attendance-vote",
          "league/team_select_modal_for_attendance_vote",{
            match : this.matches,
            teams : this.userInfo?.teams || [],
            user: this.userInfo
          }
        )
      },
      // 리그 관련 끝
      collectReportData() {
        gtag('event', 'click_sample_report', {
          'event_category': 'click_sample_report',
          'event_label': 'AI 매치 샘플 리포트 보기 클릭'
        });
      },
      getSaleType() {
        if(!this.sale_price){
          return 'none';
        }

        if(this.show_super_sub){
          return 'supersub';
        } else if(this.show_earlybird) {
          return "earlybird"
        }
        return 'none';
      },
      getEarlybirdGrade() {
        switch(this.matches.is_earlybird) {
          case 0:
            return 'C';
          case 1:
            return 'B';
          case 2:
            return 'A';
          default:
            return 'none';
        }
      },
      getIsSoldoutHurry() {
        if(this.matches.type === 'league'){
          return this.matches.confirm_cnt >= 2 ? true : false;
        }
        return this.matches.confirm_cnt >= 6 ? true : false;
      },
      sendViewMatchDetailEvent() {
        utils.common.sendHackleEvent("view_match_detail", this.matches.id, {
          ...utils.common.getMatchEventInfo(this.matches),
          previous_page: utils.common.getPreviousPath(),
          is_login: this.userInfo ? true : false,
          user_level: this.userInfo ? utils.common.getRecordLevel(this.userInfo.profile_level.tier, this.userInfo.profile_level.grade) : null,
          user_level_name: this.userInfo ? `${this.userInfo.profile_level.tier_ko}${this.userInfo.profile_level.grade || ''}` : "none",
          match_sale: this.sale_price,
          match_sale_type: this.getSaleType(),
          is_earlybird: this.show_earlybird,
          earlybird_grade: this.getEarlybirdGrade(),
          match_name: title,
        });
      },
      handleCopy() {
        gtag('event', 'copy_my_invite_link', {
          'match_id': matchID,
          'user_id': user_id,
        });

        Clipboard.copy(`${location.host}/player/${this.userInfo.user_cd}`);
        alert("주소가 복사됐어요!")
      }
    },
    computed : {
      renderReportLink() {
        return window.location.hostname === 'www.plabfootball.com'
            ? 'https://refutsal.com/ai-report/sample-report'
            : 'https://webapp-refutsal-front-git-dev-gwyuns-projects.vercel.app/ai-report/sample-report';
      },
      label_playtime(){
        if (this.matches.type == 'league') {
          return "팀"
        }
        else {
          let time = this.matches.playtime + "시간"
          if(this.matches.playminute > 0){
            time += " " + this.matches.playminute + "분"
          }
          return time
        }
      },
      label_level(){
        if (this.matches.type === 'starter') {
            return "아마추어2 이하"
        }

        switch(this.matches.level) {
            case 1:
                return '초급';
            case 2:
                return '중급';
            case 3:
                return '아마추어2 이하';
            case 4:
                return '아마추어3 이하';
            case 5:
                return '아마추어4 이상';
            default:
                return '모든 레벨';
        }
      },
      label_sex(){
        if(this.matches.sex == -1){
            return "여자만"
        }
        else if(this.matches.sex == 1){
            return "남자만"
        }
        else{
          return "남녀 모두"
        }
      },
      label_stud(){
        if(this.matches.type == 'starter'){
          return "풋살화/운동화"
        }
        else if(this.matches.stud == 'ALL'){
          return "풋살화/AG, MG 스터드"
        }
        else{
          return "풋살화/운동화"
        }
      },
      woman_player_cnt(){
        let cnt = 0
        this.matches.applys.forEach((item,idx) => {
            if(item.status=='CONFIRM' && item.user_sex == -1 )  cnt++
        })
        return cnt
      },
      label_match_type(){
        if(this.matches.type === "tshirt" || this.matches.type === 'match'){
          return "2파전"
        } else if(this.matches.type === "3teams"){
          return "3파전"
        }
        return "3파전"
      },
      label_manager(){
        if(this.matches.manager_id == 102){
          return "매니저가 미정이에요"
        }
        else{
          return this.matches.label_manager_name + " 매니저가 진행해요"
        }
      },
      leadPlaberCnt(){
        return !!this.leadPlaberInfo.lead_plaber + !!this.leadPlaberInfo.standby_lead_plaber
      },
      label_parking(){
        if(this.matches.is_parking_free){
          return "무료주차"
        }
        else if(this.matches.is_parking){
          return "유료주차"
        }
        else{
          return "주차장"
        }
      },
      label_toilet(){
        if(this.matches.is_toilet == 1){
          return "남녀 구분"
        }
        else{
          return "화장실"
        }
      },
      tshirt_team_light() {
        if (this.matches.teams != undefined) {
          let team = this.matches.teams.find((team) => team.color == "LIGHT");
          if (team != undefined) {
            return team;
          }
        }
        return null;
      },
      tshirt_team_dark() {
        if (this.matches.teams != undefined) {
          let team = this.matches.teams.find((team) => team.color == "DARK");
          if (team != undefined) {
            return team;
          }
        }
        return null;
      },
      tshirt_player_light() {
        if (this.tshirt_team_light != null) {
          return this.tshirt_team_light?.match_player.length;
        }
        return 0;
      },
      tshirt_player_dark() {
        if (this.tshirt_team_dark != null) {
          return this.tshirt_team_dark?.match_player.length;
        }
        return 0;
      },
      formattedMatchFullDate(){
        return this.isoDateToKST(this.matches.schedule, "M월 D일 dd요일 HH:mm")
      },
      isShowAttendanceVoteBanner() {
        return (
          this.matches.type == 'league' &&
          this.userInfo?.teams?.length > 0 &&
          moment() < moment(this.matches.schedule)
        )
      },
      computedAverageLevel() {
        return this.levelGraphData?.average_level || '-';
      },
      matchLevels() {
        const labels = {
          'rookie': '루키',
          'beginner': '비기너',
          'amateur': '아마추어',
          'semipro': '세미프로',
          'pro': '프로'
        };
          
        return Object.keys(labels).map(key => {
          const gradeValue = this.levelGraphData?.grades?.[key];
          return {
            key: key,
            label: labels[key],
            value: gradeValue !== undefined ? gradeValue : 0
          };
        });
      },
      isShowAverageLevel() {
        return this.levelGraphData?.average_level !== '루키';
      },
      convertUrlsToLinks() {
        const urlPattern = /(https?:\/\/[^\s]+)/g;

        return this.matches.intro.replace(urlPattern, url => {
          try {
            const validatedUrl = new URL(url);
            if (!['http:', 'https:'].includes(validatedUrl.protocol)) {
              return url;
            }
            return `<a href="${validatedUrl.href}" target="_blank" class="match-info__link">AI 리포트 샘플 보기</a>`;
          } catch (e) {
            return url;
          }
        });
      },
      showReportDescription() {
        return this.matches.is_replab && moment(this.matches.schedule) < moment("2025-03-10");
      }
    },
    watch : {
      playStyle_team(){
        var playStyles = []
        if(this.playStyle_team != null){
          var team = this.teams.find(item => item.id == this.playStyle_team)
          team.match_player.forEach((player,index) => {
            var player = this.players.find(item => item.user_id == player.user_id)
            if(player != undefined){
                playStyles.push(this.setPlayStylePerPlayer(player))
            }
          })
        }
        else{
          this.players.forEach((player,index) => {
            if(player.status == 'CONFIRM') {
              playStyles.push(this.setPlayStylePerPlayer(player))
            }
          })
        }
        this.setPlayStyleInfo(playStyles)
        this.updatePlayStyleGraph()
      },
      show_league_apply_modal() {
          if(this.show_league_apply_modal) {
              document.body.style.overflow = 'hidden'
              return
          }
          document.body.style.overflow = 'auto'
      },
      tshirt_light_modal(newValue) {
        if (newValue === true) {
          this.fetchLevelGraphData('light');
        }
      },
      tshirt_dark_modal(newValue) {
        if (newValue === true) {
          this.fetchLevelGraphData('dark');
        }
      }
    }
  })
</script>

