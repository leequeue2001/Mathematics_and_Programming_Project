         axios.get('/api/v2/matches/'+matchID+'/plaber_of_the_match/', config)
         .then((res) => {
           if(res.data.result != undefined){
              this.pom = res.data.result
           }
         })
         .catch((err) => {console.log(err)})
      },
      fetchTeams() {
        axios.get('/api/v2/matches/'+matchID+'/teams/', config)
        .then((res) => {
          if(res.data != undefined){
            this.teams = res.data
            if(this.teams.length > 0){
              if(this.teams[0].level != null){
                this.showTeamLevel = true
              }
            }
          }
        })
        .catch((err) => {console.log(err)})
      },
      fetchExtraInfo(){
        axios.get('/api/v2/matches/'+matchID+'/extra/', config)
        .then((res) => {
          if(res.data.result){
            this.pom_cnt = res.data.pom_count
            Object.keys(res.data.all_user_level).forEach((key) => {
              this.playStyleAllUsers.push(res.data.all_user_level[key])
            })
            this.updatePlayStyleGraph()
          }
        })
        .catch((err) => {console.log(err)})
      },
