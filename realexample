<script type="text/javascript">
  var covidTrainList = [45389, 45390, 45391, 45553, 45554, 45555, 45556, 45557, 45558];
  var covidTrain
  if(covidTrainList.includes(591326) == true) {
    covidTrain = true
  } else {
    covidTrain = false
  };


  function initmap(){
    if (typeof kakao != "undefined") {
      var lat = 37.480403;
      var lng = 126.886872;
      var container = document.getElementById('map');

      var options = {
        center: new kakao.maps.LatLng(lat, lng),
        level: 3
      };
      var map = new kakao.maps.Map(container, options);
      var marker = new kakao.maps.Marker({
          // 지도 중심좌표에 마커를 생성합니다
          position: map.getCenter()
      });
      // 지도에 마커를 표시합니다
      marker.setMap(map);
    }
  }
  function showmap() {
    if (window.kakao && window.kakao.maps) {
      initmap();
      var togglemap = document.getElementById("toggleMap");
      var stadmap = document.getElementById("stadMap");
      if (stadmap.style.zIndex=== "0") {
        stadmap.style.zIndex = "1";
        togglemap.innerHTML = "지도 닫기";
      } else {
        stadmap.style.zIndex = "0";
        togglemap.innerHTML = "지도 보기";
      }
    } else {
      const script = document.createElement("script");
      script.async = true;
      script.src =
        "https://dapi.kakao.com/v2/maps/sdk.js?appkey=865f4b3b9dc06d856cd4dac8406f5c2c&autoload=false";
      document.head.appendChild(script);
      script.addEventListener("load", () => {
        window.kakao.maps.load(() => {
          showmap()
        });
      });
    }
  }
  function showGallery() {
    var modalStadGal = document.getElementById('modal__stadium-gallery');
    modalStadGal.style.display = 'block';
    document.body.style.overflow = 'hidden'
    return
  }
  function closeGallery() {
    var modalStadGal = document.getElementById('modal__stadium-gallery');
    modalStadGal.style.display = 'none';
    document.body.style.overflow = 'auto'
  }
</script>
