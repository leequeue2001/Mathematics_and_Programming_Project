    <script type="text/javascript">
      var modalRegist = document.getElementById("modalRegist");
      var openRegist = document.getElementById("regist");
      var openLogin = document.getElementById("login");
      AppleID.auth.init({
        clientId : 'com.myplaycompany.signin',
        scope : 'name email',
        redirectURI : (utils.app.isApp() && /android/i.test(navigator.userAgent)) ? `${window.location.origin}/api/v2/apple-redirect/` : window.location.origin,
        state : 'signin',
        usePopup : !(utils.app.isApp() && /android/i.test(navigator.userAgent))
      });
      window.onclick = function(event) {
        if (event.target == modalRegist) {
          modalRegist.style.display = "none";
          hiddenScroll.style.overflow = "inherit";
        }
      }
      var hiddenScroll = document.body;
      function openKakaoRegist(is_event=0) {
        is_coupon_event = is_event
        modalRegist.style.display = "block";
        hiddenScroll.style.overflow = "hidden";
      }
      if(window.Kakao){
        Kakao.init('865f4b3b9dc06d856cd4dac8406f5c2c');
      }
      function pressedLoginKakao(kakao_type='check', cash=0, redirect_uri=null) {
        Kakao.Auth.login({
          success: function(authObj) {
            access_token = authObj['access_token']
            Kakao.Auth.setAccessToken(access_token);
            getKakaoAccount(kakao_type, cash, redirect_uri);
          },
          fail: function(err) {
            console.log(err)
          }
        });
      }
      function getKakaoAccount(kakao_type, cash=0, redirect_uri=null) {
        Kakao.API.request({
          url: '/v2/user/me',
          success: function(res) {
            var kakao_id = res['id']
            $.ajax({
              type: "POST",
              url: "/kakao_sync/",
              data: {
                'json': JSON.stringify(res),
                'kakao_type': kakao_type,
                'cash': cash,
                'csrfmiddlewaretoken': 'R0lPWJDIpRWYPygm2CREowK0HQ3E5C7AsazpNvwGq6qLG9XMWxbsQykmzMfypBVb',
                'is_coupon_event': is_coupon_event,
              },
              dataType: "json",
              success: function(response) {
                if (redirect_uri != null) {
                  utils.webView.navigate(redirect_uri);
                  return
                }
                var type = response.type
                if (type == 'kakao') {
                  utils.webView.navigate("/");
                } else if (type == 'email') {
                  utils.webView.navigate("/kakao_sync/");
                } else if (type == 'regist') {
                  utils.webView.navigate("/auth/register/complete");
                } else if (type == 'sync_login') {
                  alert('이메일 계정으로 로그인 하시면 카카오 로그인이 완료됩니다.')
                  utils.webView.navigate("/accounts/login/");
                } else if (type == 'home') {
                  utils.webView.navigate("/");
                } else if (type == 'exist_user') {
                  alert('이미 가입 하셨습니다')
                  utils.webView.navigate("/mypage/");
                } else if (type == 'check_error') {
                  alert(response.msg)
                } else {}
              },
              error: function(response) {
                alert('카카오 로그인에 실패 하였습니다.\n고객센터로 문의 주세요.')
              },
            })
          },
          fail: function(error) {
            alert('카카오톡 사용자 정보를 가져오는 데 실패했습니다.\n고객센터로 문의주세요.')
          }
        });
      }

      document.addEventListener('AppleIDSignInOnSuccess', (event) => {
        axios.post('/api/v2/apple-oauth/', event.detail, config)
          .then(({data}) => {
            switch (data.user_status) {
              case 'NOT_FOUND_USER':
                utils.webView.navigate(`/auth/register?${new URLSearchParams(data.data).toString()}`);
                break;
              case 'REGISTERED_USER':
                utils.webView.navigate("/");
                break;
              default:
                break;
            }
        });
      });

      async function handleAppleLogin() {
        try {
          await AppleID.auth.signIn();
        } catch (error) {
          alert('애플 로그인에 실패하였습니다.\n고객센터로 문의해 주세요.');
        }
      }
      
    </script>
