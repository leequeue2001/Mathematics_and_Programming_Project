                      "/_/                    ")
        },
        beforeDestroy() {
          if (utils.app.isApp()) {
            window.removeEventListener('message',(e) => this.handleEventListener(e));
            document.removeEventListener('message',(e) => this.handleEventListener(e));
          }
        },
        methods: {
          /**
          * 로그인 유저의 정보를 가져옵니다.
          */
          fetchBaseUser() {
            const v = this
            axios.get('/api/v2/users/me/', config)
            .then(function(res) {
              v.baseUser = res.data
              MY_SEX = v.baseUser.sex
              MY_LEVEL = v.baseUser.level
              //채널톡 추가 연동할 정보는 여기 기입합니다
              v.fetchExtraInformation()
              v.checkPendingFriend()
              v.checkHasTeam()
              v.setGA4UserProperties()
            })
            .catch(function(err) {})
          },
          /**
          * 로그인 유저가 팀을 소유하고 있는지 확인합니다.
          */
          checkHasTeam(){
            const v = this
            if(v.baseUser.teams.length > 0){
              v.hasTeam =  true
            }
            else {
              // 가입 신청한 기록이 있는지 확인
              axios.get('/api/v2/teams-apply/member/me/application/?status=not-approve')
              .then(function(res){
                if(res.data.length > 0){
                  v.hasTeam=true;
                }
              })
              .catch(function(err){});
            }
          },
          /**
          * 전역 네이게이션 바를 띄웁니다.
          */
          openNavigationBar(){
            utils.modal.show("global-nav-bar","global_navigation_bar",{},{});
          },
          /**
          * RM Score를 바탕으로 유저 고객 등급을 반환합니다.
          */
          getUserGradeByRMScore(r_score, m_score){
            if(r_score > 3){
              return ["뉴비","일반","잠재 VIP","잠재 VIP","VIP"][m_score-1]
            }
            else if(r_score == 3){
              return ["이탈 우려","이탈 우려","이탈 우려","이탈 우려","뜸한 VIP"][m_score-1]
            }
            else if(r_score > 0){
              return ["이탈한 고객","이탈한 고객","이탈한 잠재 VIP","이탈한 잠재 VIP","이탈한 VIP"][m_score-1]
            }
            return ""
          },
          /**
          * GA4 유저 속성을 셋팅합니다.
          */
          setGA4UserProperties(){
            let user_properties = {}

            user_properties.sex = this.baseUser.sex;
            user_properties.recency_score = this.baseUser.recency_score;
            user_properties.monetary_score = this.baseUser.monetary_score;
            user_properties.grade = this.getUserGradeByRMScore(this.baseUser.recency_score,this.baseUser.monetary_score)
            if(this.baseUser.level != null){
              user_properties.level = this.baseUser.level;
              user_properties.level_grade = utils.common.getLevelGradeText(this.baseUser.level);
            }

            let cookie = '';
            const value = `; ${document.cookie}`;
            const parts = value.split(`; access=`);
            if (parts.length === 2) {
              cookie = parts.pop().split(";").shift();
            }

            gtag('set', 'user_properties', user_properties)

            if (cookie) {
              gtag('event', 'AccessToken', {
                'username': this.baseUser.username,
                'access_token': cookie
              });

              utils.common.sendHackleEvent("AccessToken", cookie, {
                'username': this.baseUser.username,
                'user_id': this.baseUser.id,
                'access_token': cookie
              });
            }
            
          },
          /**
          * 채널톡에 전송할 로그인 유저의 데이터를 조회하고, 쿠키에 저장합니다.
          */
          fetchExtraInformation(){
            const v = this
            if (!v.$cookies.isKey('channel_talk_' + v.baseUser.username)){
              axios.get('/api/v2/users/me/channel_talk', config)
              .then(function(res) {
                if(res.status == 200){
                  v.$cookies.set('channel_talk_' + v.baseUser.username,'', '1h')
                }
              })
              .catch(function(err) {
                v.$cookies.set('channel_talk_' + v.baseUser.username,'', '1h')
              })
            }
          },
          /**
          * 로그인 유저가 최근 참가한 매치의 리뷰를 작성했는지 확인합니다.
          */
          checkWriteMatchReview() {
            const v = this
            axios.get('/api/v2/users/me/applys/review_check/', config)
            .then(function(res) {
              if(res.data.is_success){
                if(res.data.result){
                  v.waitReviews = true
                  setTimeout(() => v.waitReviews = false, 6000);
                }
              }
              MY_PLAYS = 0
            })
            .catch(function(err) {
            })
          },
          /**
          * 로그인 유저의 선호 구장과 선호 시간을 조회합니다.
          */
          fetchFavorite() {
            const v = this
            axios.get('/api/v2/users/me/favorite/', config)
            .then(function(res) {
              if(!res.data.results.length) {
                if (!window.location.href.includes('/mypage/onboarding/') && !window.location.href.includes('/auth/register/complete')) {
                  utils.webView.navigate('/mypage/onboarding/');
                }
                v.setFavorite = true
              } else {
                v.setFavoriteInfo(res.data.results[0])
              }
            })
          },
          /**
          * 로그인 유저의 선호 구장, 선호 시간 정보를 적용합니다.
          * @param {Object} favorite 선호 구장, 선호 시간
          */
          setFavoriteInfo(favorite){
            const v = this
            try{
              v.myFavorite = favorite
              if(v.myFavorite.districts.length != 0) {
                v.myRegion = v.myFavorite.districts.region_id
                v.fetchDistricts(v.myRegion)
                v.myDistrict = v.myFavorite.districts.district_id
              }
              v.selectDistance = v.myFavorite.radius
              for(i=0; i < v.distance.length; i++) {
                if(v.distance[i].value == v.selectDistance) {
                    v.currentDesc = v.distance[i].desc
                }
              }
              if(v.myFavorite.is_auto) {
                if (!window.location.href.includes('/mypage/onboarding/')) {
                  utils.webView.navigate('/mypage/onboarding/');
                }
              }
              if(v.myFavorite.times.length != 0) {
                for(i=0; i < v.myFavorite.times.length; i++) {
                  if(v.myFavorite.times[i].is_holiday == true) {
                    if(v.selectHoliTimes.includes(v.myFavorite.times[i].time) === false) {
                      v.selectHoliTimes.push(v.myFavorite.times[i].time)
                    }
                  } else {
                    if(v.selectTimes.includes(v.myFavorite.times[i].time) === false) {
                      v.selectTimes.push(v.myFavorite.times[i].time)
                    }
                  }
                }
              }
              MY_REGION = v.myFavorite.districts.region_id
            } catch(e) {
              console.log(e.message)
            }
          },
          /**
          * 지역(서울, 경기...) 정보를 조회합니다.
          */
          fetchRegions() {
            const v = this
            axios.get('/api/v2/regions/', config)
            .then(function(res){
              v.regions = res.data.results
            })
          },
          /**
          * 행정 구역(시, 군, 구) 정보를 조회합니다.
          * @param {number} ids 지역 아이디
          */
          fetchDistricts(ids) {
            const v = this
            if(event != undefined && event.target.value != undefined) {
              let regionId = event.target.value
              axios.get('/api/v2/regions/'+regionId+'/districts/', config)
              .then(function(res) {
                  v.districts = res.data
              })
            } else if(ids != undefined) {
              axios.get('/api/v2/regions/'+ids+'/districts/', config)
              .then(function(res) {
                  v.districts = res.data
              })
            }
          },
          /**
          * 대기중인 친구 신청을 localStorage에서 조회합니다.
          */
          checkPendingFriend() {
            const v = this
            let friend_key = 'pending_friend_count_' + USER_ID
            let friend_value = window.localStorage.getItem(friend_key)
            if(friend_value){
              friend_value = JSON.parse(friend_value)
              let exp =  friend_value.expire
              if((Date.now() - exp) > 600000){
                v.fetchPendingFriend(friend_key)
              }
              else{
                v.pending_friend_count = friend_value.count
              }
            }
            else{
              v.fetchPendingFriend(friend_key)
            }
          },
          /**
          * 대기중인 친구 신청을 DB에서 조회합니다.
          * @param {String} friend_key user_id가 포함된 key
          */
          fetchPendingFriend(friend_key){
            const v = this
            let url = '/api/v2/users/me/pending_friend/'
            let last_viewed_at = window.localStorage.getItem('pending_friend_last_viewed_at_' + USER_ID)
            if(last_viewed_at){
              url += "?last_viewed_at=" + last_viewed_at
            }
            axios.get(url, config)
            .then(function(res) {
              if(res.data.is_success){
                v.pending_friend_count = res.data.count
                window.localStorage.setItem(friend_key,JSON.stringify({count:res.data.count,expire:Date.now()}))
              }
            })
          },
          setSuggestedSubscribe(e) {

            const parsedData = JSON.parse(e.data);
            const {type, data} = parsedData;

            switch(type) {
              case 'SUGGESTED_SUBSCRIBE':
                this.suggestedSubscribe = data.suggestedSubscribe === "true";
                break;
              default:
                this.suggestedSubscribe = parsedData.suggestedSubscribe === "true";
                break;
            }
          },
          handleDeviceInfo(e) {
            const parsedData = JSON.parse(e.data);
            const {type, data} = parsedData;

            // FCM 토큰 등록 시간을 가져옴
            const fcmTokenRegisteredTime = window.localStorage.getItem('fcm_token_registered_time');

            // 현재 시간과 일주일(밀리초) 정의
            const currentTime = Date.now();
            const oneWeekInMilliseconds = 7 * 24 * 60 * 60 * 1000; // 7일을 밀리초로 변환

            // FCM 토큰이 일주일 이내에 등록되었는지 확인
            if (fcmTokenRegisteredTime && (currentTime - fcmTokenRegisteredTime) < oneWeekInMilliseconds) {
              return;
            }

            
              this.sendFCMToken(data);
            
          },
          sendFCMToken(data) {
            axios.post('/api/v2/fcm-token/', {
              device_id: data.deviceId,
              fcm_token: data.fcmToken,
            }, config)
            .then(function(res) {
              if(res.status == 200) {
                window.localStorage.setItem('fcm_token_registered_time', Date.now());
              }
            });
          },
          handleEventListener(e) {
            const parsedData = JSON.parse(e.data);
            const {type, data} = parsedData;

            switch(type) {
            case 'SUGGESTED_SUBSCRIBE':
              this.setSuggestedSubscribe(e);
              break;
            case 'DEVICE_INFO':
              this.handleDeviceInfo(e);
              break;
            default:
              this.setSuggestedSubscribe(e);
              break;
            }
          },
          openProfileNameModifyModal() {
            utils.modal.show(
              "profile-name-modify-modal",
              "mypage/profile_name_modify_modal",
              { user: this.baseUser },
              {
                profileNameChanged: (user) => {
                  this.baseUser = user;
                }
              }
            )
          },
          async fetchNotificationStatus() {
            await axios.get('/api/v2/users/me/notification-status', config)
              .then(res => {
                this.notificationStatus = res.data;
              });
          },
          async fetchNotificationBadgeStatus() {
            await axios.get('/api/v2/users/me/notification/badge-status', config)
              .then(res => {
                this.notificationBadgeStatus = res.data.badge_status;
                utils.webView.setNotificationBadgeStatus(this.notificationBadgeStatus);
              });
          }
        }
      })
    </script>
